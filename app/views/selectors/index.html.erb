<% content_for(:content_title, raw("<h1>Selectors</h1>")) %>

<table class="zebra-striped">
  <thead>
  <tr>
    <th>Name</th>
    <th>Monographic Form</th>
    <th>Just Say Yes Form</th>
    <th>Admin</th>
    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% @selectors.each do |selector| %>
    <tr>
      <td><%= selector.last_first %></td>
      <td><%= selector.monographic? ? "Yes" : "No" %></td>
      <td><%= selector.just_say_yes? ? "Yes" : "No" %></td>
      <td><%= selector.admin? ? "Yes" : "No" %></td>
      <td><%= link_to("Edit", edit_selector_path(selector)) %></td>
    </tr>
  <% end %>
  </tbody>
</table>