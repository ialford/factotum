<% breadcrumb([link_to('Service Points', availability_service_points_path), link_to(@hours.service_point.name, availability_service_point_hours_path(@hours.service_point)), "New Regular Hours"]) %>

<h1>New Regular Hours</h1>
<%= simple_form_for @hours, :html => {id: "new_hours_form", class: "new_hours_form"}, :url => availability_service_point_regular_hours_path(@hours.service_point) do |f| %>
    <%= render :partial => 'availability/hours/form', :locals => { :f => f } %>
<% end %>
