<div id="<%= search_type %>SearchContainer" class="six columns search-container search-<%= search_type %>" data-target="<%= search_url %>">
  <% if Rails.env == 'development' %>
    <%= link_to "#{search_type}.json", search_url %>
  <% end %>
  <fieldset>
    <legend><%= title %></legend>
    <p class="more">1-<span class="results-count">10</span> <a href="#" class="search-link">All Results ></a></p>
    <p><span class="total-count">0</span> results</p>
    <div class="search-results">Loading results... <%= image_tag('ajax-loader-main.gif') %></div>
    <p class="more">1-<span class="results-count">10</span> <a href="#" class="search-link">All Results ></a></p>
  </fieldset>
</div>
