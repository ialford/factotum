body { background-color: white; font-family: 'Helvetica', 'Nimbus Sans','sans-serif';}
div.header { display:none; }
div.menu { display: none; }
div.breadcrumb { display: none; }
div.breadcrumb p { display: none; }
div.content { background-color: white; }
div.logged_in_user {display:none; }
div.footer { display:none; }

.print_page { width: 100%; margin: 0px; padding: 0px;}

.print_page { page-break-after:always; }
.print_page:last-child { page-break-after:auto; }

.print_page { font-size: 3em; color: 0000CC; }
.print_page h1 { text-align: center;  line-height: 1em; margin-bottom: .66em;  color: #002B5C; }
.print_page h1.long { font-size: 1.40em; }
.print_page h1.short { font-size: 1.70em; }
.print_page h1.small_margin { margin-bottom: .22em }

.print_page .hours_name { text-align: center; margin-bottom: .75em; color: #B18D03; }
.print_page .hours_row { line-height: 1.5em}

.print_page .hours_day {float:none; width:100%}
.print_page .hours_text {float:none; margin-left: 40%; width: 60%; }

.print_page .hours_append_text { font-size: .65em; line-height: 1.25em; margin-top: 50px;}
.print_page .hours_prepend_text { font-size: .65em; line-height: 1.25em; }

.print_page a { color: inherit; text-decoration: none; }

.print_page_footer { padding-top: 55px; font-size: .5em; text-align:center; }

.print_page .hours_row:nth-child(even) {color: #666; }
.print_page .hours_row:nth-child(odd) {color: black; }


.print_page .centered { text-align: center; }
